Cell[BoxData[
 RowBox[{
  RowBox[{"covderdown", "[", 
   RowBox[{"f_", ",", "vect_", ",", "adown_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "christoffel", ",", "dim", ",", "j", ",", "i", ",", "k", ",", "result", 
      ",", "sum"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"christoffel", " ", "=", " ", 
      RowBox[{"christoffelfirstup", "[", 
       RowBox[{"f", ",", "vect"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"dim", " ", "=", " ", 
      RowBox[{"Length", "[", "vect", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"result", " ", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "dim"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "dim"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "dim"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", "\t", 
       RowBox[{"For", " ", "[", 
        RowBox[{
         RowBox[{"j", " ", "=", " ", "1"}], ",", 
         RowBox[{"j", "\[LessEqual]", "dim"}], ",", 
         RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", "\t\t", 
         RowBox[{
          RowBox[{"sum", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
          "\t\t", 
          RowBox[{"For", "[", 
           RowBox[{
            RowBox[{"k", "=", "1"}], ",", 
            RowBox[{"k", "\[LessEqual]", "dim"}], " ", ",", 
            RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", "\t\t\t", 
            RowBox[{
             RowBox[{"sum", " ", "=", " ", 
              RowBox[{"sum", " ", "+", " ", 
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{"christoffel", "[", 
                   RowBox[{"[", "k", "]"}], "]"}], "[", 
                  RowBox[{"[", "i", "]"}], "]"}], "[", 
                 RowBox[{"[", "j", "]"}], "]"}], "*", 
                RowBox[{"adown", "[", 
                 RowBox[{"[", "k", "]"}], "]"}]}]}]}], ";"}]}], 
           "\[IndentingNewLine]", "\t\t\t\t", "]"}], ";", 
          "\[IndentingNewLine]", "\t\t", 
          RowBox[{
           RowBox[{
            RowBox[{"result", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "[", 
            RowBox[{"[", "j", "]"}], "]"}], " ", "=", " ", 
           RowBox[{"FullSimplify", "[", 
            RowBox[{
             RowBox[{"D", "[", 
              RowBox[{
               RowBox[{"adown", "[", 
                RowBox[{"[", "j", "]"}], "]"}], ",", 
               RowBox[{"vect", "[", 
                RowBox[{"[", "i", "]"}], "]"}]}], "]"}], "-", "sum"}], 
            "]"}]}], ";"}]}], "\[IndentingNewLine]", "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "result", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
