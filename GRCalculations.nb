Cell[BoxData[
 RowBox[{
  RowBox[{"expansion", "[", 
   RowBox[{"f_", ",", "vect_", ",", "aup_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"temp", ",", "trace", ",", "i"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"temp", " ", "=", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{"covderup", "[", 
        RowBox[{"f", ",", "vect", ",", "aup"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"trace", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", 
        RowBox[{"Length", "[", "vect", "]"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", "\t", 
       RowBox[{"trace", " ", "=", " ", 
        RowBox[{"trace", " ", "+", " ", 
         RowBox[{
          RowBox[{"temp", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "[", 
          RowBox[{"[", "i", "]"}], "]"}]}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"trace", " ", "=", " ", 
      RowBox[{"FullSimplify", "[", "trace", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "trace", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
